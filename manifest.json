{
    "name" : "AutomaticXpathGenerator",
    "version" : "1.0",
    "description" : "This will generate xpaths for elements on the webpage",
    "manifest_version" : 2,
    "content_scripts" : [
        {
            "matches" : ["<all_urls>"],
            "js" : ["content.js"]
        }
    ],
    "permissions": ["storage", "activeTab"],
    "browser_action" : {
        "default_popup" : "demo.html",
        "default_icon" : "logo.png"
    }
}